{
    "name": "planeat",
    "private": true,
    "version": "0.0.1",
    "engines": {
        "node": "22"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "build:analyze": "cross-env ANALYZE=true pnpm build",
        "build:size": "npm run build && size-limit",
        "start": "next start",
        "test": "jest",
        "test:e2e": "playwright test ./e2e",
        "test:e2e:watch": "playwright test --ui ./e2e",
        "test:coverage": "jest --coverage",
        "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
        "test:watch": "jest --watch",
        "sync:types:local": "supabase gen types typescript --local > ./src/types/supabase.ts && prettier --write ./src/types/supabase.ts",
        "lint": "eslint --cache --ignore-path .gitignore .",
        "prettier": "prettier .",
        "check:format": "pnpm prettier --list-different",
        "check:types": "tsc --noEmit",
        "format": "pnpm prettier --write",
        "prepare": "husky install",
        "validate": "pnpm lint && pnpm check:format && pnpm check:types"
    },
    "dependencies": {
        "@mantine/core": "7.1.7",
        "@mantine/dates": "7.1.7",
