        "next-router-mock": "0.9.10",
        "prettier": "3.0.3",
        "size-limit": "10.0.1",
        "supabase": "1.106.1",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.1",
        "typescript": "5.2.2"
    }
