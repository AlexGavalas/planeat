        "@supabase/auth-helpers-react": "0.4.2",
        "@supabase/supabase-js": "2.38.4",
        "@tanstack/react-query": "5.0.5",
        "cheerio": "1.0.0-rc.12",
        "date-fns": "2.30.0",
        "date-fns-tz": "2.0.0",
        "dayjs": "1.11.10",
        "formidable": "3.5.1",
        "i18next": "23.6.0",
        "iconoir-react": "6.1.1",
        "jotai": "2.5.0",
        "lodash": "4.17.21",
        "mammoth": "1.6.0",
        "next": "14.0.0",
        "next-auth": "4.24.4",
        "next-i18next": "14.0.3",
        "react": "18.2.0",
        "react-dnd": "16.0.1",
        "react-dnd-html5-backend": "16.0.1",
